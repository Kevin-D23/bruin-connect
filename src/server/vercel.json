{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
   "routes": [
     {
       "src": "/api/song/(.*)",
       "dest": "/index.js"
     },
     {
       "src": "/api/album/(.*)",
       "dest": "/index.js"
     },
     {
       "src": "/(.*)",
       "dest": "/index.js"
     }
   ]
  }
  